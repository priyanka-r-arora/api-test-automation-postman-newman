  "item": [
    {
      "name": "User Lifecycle Journey",
      "item": [
        {
          "name": "Journey Start: Register New User",
          "event": [
            {
              "listen": "prerequest",
              "script": {
                "exec": [
                  "// Generate unique user for E2E journey",
                  "const timestamp = Date.now();",
                  "const journeyEmail = `e2e.journey.${timestamp}@automation.test`;",
                  "pm.collectionVariables.set('e2eUserEmail', journeyEmail);",
                  "",
                  "console.log('[E2E JOURNEY] Starting user lifecycle test');",
                  "console.log(`[E2E JOURNEY] User email: ${journeyEmail}`);"
                ],
                "type": "text/javascript"
              }
            },
            {
              "listen": "test",
              "script": {
                "exec": [
                  "pm.test('User registration successful (201)', function () {",
                  "    pm.response.to.have.status(201);",
                  "});",
                  "",
                  "pm.test('User receives unique ID', function () {",
                  "    const jsonData = pm.response.json();",
                  "    pm.expect(jsonData.id).to.be.a('number');",
                  "    pm.expect(jsonData.id).to.be.above(0);",
                  "});",
                  "",
                  "pm.test('User is created as active', function () {",
                  "    const jsonData = pm.response.json();",
                  "    pm.expect(jsonData.status).to.equal('active');",
                  "});",
                  "",
                  "// Store for journey continuation",
                  "const userData = pm.response.json();",
                  "pm.collectionVariables.set('e2eUserId', userData.id);",
                  "pm.environment.set('e2eUserId', userData.id);",
                  "",
                  "console.log(`[E2E JOURNEY] User registered with ID: ${userData.id}`);"
                ],
                "type": "text/javascript"
              }
            }
          ],
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n    \"name\": \"E2E Journey Test User\",\n    \"email\": \"{{e2eUserEmail}}\",\n    \"gender\": \"female\",\n    \"status\": \"active\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/users",
              "host": [
                "{{baseUrl}}"
              ],
              "path": [
                "users"
              ]
            }
          },
          "response": []
        },
        {
          "name": "Journey: View User Profile",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "pm.test('Profile retrieved successfully (200)', function () {",
                  "    pm.response.to.have.status(200);",
                  "});",
                  "",
                  "pm.test('Profile matches registration data', function () {",
                  "    const jsonData = pm.response.json();",
                  "    pm.expect(jsonData.name).to.equal('E2E Journey Test User');",
                  "    pm.expect(jsonData.gender).to.equal('female');",
                  "    pm.expect(jsonData.status).to.equal('active');",
                  "});",
                  "",
                  "pm.test('Email matches registered email', function () {",
                  "    const jsonData = pm.response.json();",
                  "    const expectedEmail = pm.collectionVariables.get('e2eUserEmail');",
                  "    pm.expect(jsonData.email).to.equal(expectedEmail);",
                  "});",
