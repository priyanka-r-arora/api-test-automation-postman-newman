                  "pm.test('Returns 10 users', function () {",
                  "    const jsonData = pm.response.json();",
                  "    pm.expect(jsonData.length).to.equal(10);",
                  "});",
                  "",
                  "pm.test('User has complex address structure', function () {",
                  "    const jsonData = pm.response.json();",
