              "listen": "test",
              "script": {
                "exec": [
                  "pm.test('ReqRes API is responsive (200)', function () {",
                  "    pm.response.to.have.status(200);",
                  "});",
                  "",
                  "pm.test('Response has pagination info', function () {",
                  "    const jsonData = pm.response.json();",
                  "    pm.expect(jsonData).to.have.property('page');",
